{
  "name": "backend-optimized",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon --exec node app.js",
    "migrate:dev": "npx sequelize db:migrate --env=\"development\" --migrations-path=\"./migrations/\"",
    "migrate:test": "npx sequelize db:migrate --env=\"test\" --migrations-path=\"./migrations/\"",
    "migrate:prod": "npx sequelize db:migrate --env=\"production\" --migrations-path=\"./migrations/\""
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.4.4",
    "express": "~4.16.1",
    "express-session": "^1.18.0",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "mariadb": "^3.2.3",
    "morgan": "~1.9.1",
    "mysql": "^2.18.1",
    "mysql2": "^3.9.2",
    "nodemon": "^3.0.3",
    "sequelize": "^6.37.1",
    "sequelize-cli": "^6.6.2",
    "socket.io": "^4.7.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "web3": "^4.7.0",
    "ws": "^8.16.0"
  }
}
